import {
  {{ names.jsonRequest }},
  {{ names.operation }},
} from '{{ names.commonUtils }}';
import { NextFunction, Response, Request } from 'express';
import { OperationResolver } from '@moralisweb3/api-utils';
import Moralis from 'moralis';

export const {{ names.resolver }}  = async (
  req: Request<{{ names.jsonRequest }}>,
  res: Response,
  next: NextFunction,
) => {
  try {
    const { raw } = await new OperationResolver({{ names.operation }}, Moralis.EvmApi.baseUrl, Moralis.Core).fetch(
      {{ names.operation }}.deserializeRequest(req.params, Moralis.Core),
    );

    return res.send(raw);
  } catch (error) {
    return next(error);
  }
};